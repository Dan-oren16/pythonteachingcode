    "        print(\"Red\")\n",
    "    elif fav_color.upper() == \"O\":\n",
    "        print(\"Orange\")\n",
    "    elif fav_color.upper() == \"Y\":\n",
    "        print(\"Yellow\")\n",
    "    elif fav_color.upper() == \"G\":\n",
    "        print(\"Green\")\n",
    "    elif fav_color.upper() == \"B\":\n",
    "        print(\"Blue\")\n",
    "    elif fav_color.upper() == \"I\":\n",
    "        print(\"Indigo\")\n",
    "    elif fav_color.upper() == \"V\":\n",
    "        print(\"Violet\")\n",
    "    else:\n",
    "        print(\"No match\")\n",
    "    return\n",
    "rainbow_color(fav_color)"
